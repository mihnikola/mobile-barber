{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "app/(tabs)/employers.jsx",
    "app/(tabs)/explore.jsx",
    "app/(tabs)/index.tsx",
    "app/(tabs)/notifications.jsx",
    "context/ReservationContext.jsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
